{
	"manifest_version": 2,
	"name": "臺灣郵遞區號查詢",
	"description": "臺灣郵遞區號查詢",
	"version": "1.0",
	"permissions": [
		"storage"
	],
	"icons": {
		"16": "img/icon16.png",
		"32": "img/icon32.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"48": "img/icon48.png",
			"128": "img/icon128.png"
		},
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": [
		"img/icon128.png"
	]
}
